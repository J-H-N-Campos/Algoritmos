public class Atividade11
{
  public static void main (String[] args)
  {
    int[] vetor=new int[5];
    int[] vetora=new int[5];
    
    for(int i=0; i<5; i++)
    {
      if (vetor[i]%2==0)
      {
        System.out.println("numeros pares:"+vetor[i]);
      }else
        System.out.println("numeros impares:"+vetor[i]);
    }
  }
}
        
      