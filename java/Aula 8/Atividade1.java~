public class Atividade1
{
  public static void main (String[] args)
  {
    int nota1=0;
    int nota2=0;
    int nota3=0;
    int nota4=0;
    int nota5=0;
    double media=0;
    double soma=0;
    
    nota1=Entrada.leiaInt("digite a nota1:");
    nota2=Entrada.leiaInt("digite a nota2:");
    nota3=Entrada.leiaInt("digite a nota3:");
    nota4=Entrada.leiaInt("digite a nota4:");
    nota5=Entrada.leiaInt("digite a nota5:");
    
    soma= (nota1+nota2+nota3+nota4+nota5);
    media= (soma/5);
    
    if (media<6)
    {
      System.out.println("o aluno está reprovado por não atingir a média mínima");
    }else
      System.out.println("o aluno está aprovado");
  }
}
    
    